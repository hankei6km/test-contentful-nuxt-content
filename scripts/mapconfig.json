{
  "passthruUnmapped": false,
  "media": {
    "image": {
      "library": [
        {
          "src": "https://images.microcms-assets.io/assets/",
          "kind": "imgix"
        }
      ]
    }
  },
  "transform": "data.docsThemeCollectionCollection.items.collectionCollection",
  "flds": [
    {
      "srcName": "id",
      "dstName": "id",
      "fldType": "id"
    },
    {
      "srcName": "title",
      "dstName": "title",
      "fldType": "string"
    },
    {
      "srcName": "content",
      "dstName": "content",
      "fldType": "html",
      "convert": "markdown",
      "toMarkdownOpts": {
        "embedImgAttrs": [
          {
            "baseURL": "https://images.ctfassets.net/",
            "embedTo": "alt"
          }
        ]
      }
    },
    {
      "srcName": "description",
      "dstName": "description",
      "fldType": "string"
    },
    {
      "srcName": "category.title",
      "dstName": "category",
      "fldType": "string"
    },
    {
      "srcName": "fullscreen",
      "dstName": "fullscreen",
      "fldType": "boolean"
    },
    {
      "srcName": "version",
      "dstName": "version",
      "fldType": "string"
    },
    {
      "srcName": "menuTitle",
      "dstName": "menuTitle",
      "fldType": "string"
    },
    {
      "srcName": "subtitle",
      "dstName": "subtitle",
      "fldType": "string"
    },
    {
      "srcName": "badge",
      "dstName": "badge",
      "fldType": "string"
    },
    {
      "srcName": "list",
      "dstName": "list",
      "fldType": "object"
    }
  ]
}